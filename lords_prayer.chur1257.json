  "sentences": [
    {
      "cyrillic": "отьчє нашь·",
      "ipa": "otɪtʃe naʃɪ",
      "transcription": "otĭče našĭ",
      "translation": "Our father"
    },
    {
      "cyrillic": "ижє ѥси на нєбєсѣхъ:",
      "ipa": "jɪʒe jesi na nebesæxɯ",
      "transcription": "Iže jesi na nebesěxŭ.",
      "translation": "Thou who art in the heavens."
    },
    {
      "cyrillic": "да свѧтитъ сѧ имѧ твоѥ·",
      "ipa": "da svẽtitɯ sẽ jɪmẽ tvoje",
      "transcription": "Da svętitŭ sę imę tvoje",
      "translation": "May hallowed be thy name"
    },
    {
      "cyrillic": "да придєтъ цѣсар҄ьствиѥ твоѥ·",
      "ipa": "da pridetɯ tsæsarʲɪstvije tvoje",
      "transcription": "da pridetŭ cěsar'ĭstvije tvoje",
      "translation": "may come thy empire"
    },
    {
      "cyrillic": "да бѫдєтъ волꙗ твоꙗ",
      "ipa": "da bɔ̃detɯ volʲa tvoja",
      "transcription": "da bǫdetŭ volja tvoja",
      "translation": "may become thy will"
    },
    {
      "cyrillic": "ꙗко на нєбєси и на ꙁємл҄и:",
      "ipa": "jako na nebesi i na zemlʲi.",
      "transcription": "jako na nebesi i na zeml'i.",
      "translation": "as in heaven, also on Earth."
    },
    {
      "cyrillic": "хлѣбъ нашь насѫщьнꙑи",
      "ipa": "xlæbɯ naʃɪ nasɔ̃ʃtɪnɨjɪ",
      "transcription": "hlěbŭ našĭ nasǫštĭnyi",
      "translation": "Our supersubstantial bread"
    },
    {
      "cyrillic": "даждь намъ дьньсь·",
      "ipa": "daʒdɪ namɯ dɪnɪsɪ",
      "transcription": "daždĭ namŭ dĭnĭsĭ",
      "translation": "give us this day"
    },
    {
      "cyrillic": "и отъпоусти намъ длъгꙑ нашѧ",
      "ipa": "i otɯpusti namɯ dlɯgɨ naʃẽ",
      "transcription": "i otŭpusti namŭ dlŭgy našę",
      "translation": "and release us of our debts"
    },
    {
      "cyrillic": "ꙗко и мꙑ отъпоущаѥмъ",
      "ipa": "jako i mɨ otɯpuʃtajemɯ",
      "transcription": "jako i my otŭpuštajemŭ",
      "translation": "as we also release"
    },
    {
      "cyrillic": "длъжьникомъ нашимъ·",
      "ipa": "dlɯʒɪnikomɯ naʃimɯ.",
      "transcription": "dlŭžĭnikomŭ našimŭ",
      "translation": "our debtors,"
    },
    {
      "cyrillic": "и нє въвєди насъ въ искоушєниѥ·",
      "ipa": "i ne vɯvedi nasɯ vɯ jɪskuʃenije",
      "transcription": "i ne vŭvedi nasŭ vŭ iskušenije",
      "translation": "and do not lead us to temptation"
    },
    {
      "cyrillic": "нъ иꙁбави нꙑ отъ нєприꙗꙁни:",
      "ipa": "nɯ izbavi nɨ otɯ neprijazni,",
      "transcription": "nŭ izbavi ny otŭ neprijazni.",
      "translation": "but free us from the evil."
    },
    {
      "cyrillic": "ꙗко твоѥ ѥстъ цѣсар҄ьствиѥ",
      "ipa": "jako tvoje jestɯ tsæsarʲɪstvije",
      "transcription": "jako tvoje jestŭ cěsar'ĭstvije",
      "translation": "As thine is the empire"
    },
    {
      "cyrillic": "и сила и слава въ вѣкꙑ вѣкомъ",
      "ipa": "i sila i slava vɯ vækɨ vækomɯ",
      "transcription": "i sila i slava vŭ věky věkomŭ.",
      "translation": "and the power and the glory unto the ages of ages."
    },
    {
      "cyrillic": "аминь჻",
      "ipa": "aminɪ.",
      "transcription": "aminĭ.",
      "translation": "Amen."
    }
  ]
}